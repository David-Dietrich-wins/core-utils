{
  "name": "@digicrewllc/core-utils",
  "version": "2.2.0",
  "description": "Core helper utilities for supporting TypeScript and JavaScript projects.",
  "private": false,
  "type": "module",
  "exports": "./dist/index.mjs",
  "scripts": {
    "build": "rimraf dist && tsc",
    "build:clean": "npm run clean && npm run build",
    "build:clean:test": "npm run clean && npm run build:test:base && npm run test:coverage",
    "build:test": "npm run build:test:base && npm run test:coverage",
    "build:test:base": "rimraf dist && tsc -p tsconfig.test.json",
    "clean": "npm run clean:files && npm install",
    "clean:files": "rimraf .next _next build dist lib node_modules out package-lock.json pnpm-lock.yaml yarn.lock",
    "export:package": "echo '{\"name\": \"@digicrewllc/core-utils\", \"type\": \"module\", \"exports\": \"./dist/index.mjs\", \"version\": \"2.1.0\"}' > dist/package.json",
    "lint": "eslint ./",
    "lint:fix": "eslint ./ --fix",
    "postbuild": "cp package.json ./dist",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "test2": "jest --coverage",
    "test:coverage": "node --experimental-vm-modules node_modules/.bin/jest --coverage",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/David-Dietrich-wins/core-utils.git"
  },
  "engines": {
    "node": ">=20.0"
  },
  "keywords": [
    "GrayArrow",
    "utilities",
    "TypeScript"
  ],
  "author": "David Dietrich",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/David-Dietrich-wins/core-utils/issues"
  },
  "homepage": "https://github.com/David-Dietrich-wins/core-utils#readme",
  "dependencies": {
    "moment": "latest",
    "zod": "latest"
  },
  "devDependencies": {
    "@eslint/js": "latest",
    "@types/jest": "latest",
    "@types/jsonwebtoken": "latest",
    "@types/node": "latest",
    "@types/tmp": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "latest",
    "eslint-plugin-jest": "latest",
    "jest": "latest",
    "msw": "latest",
    "tmp": "latest",
    "ts-jest": "latest",
    "ts-node": "latest",
    "typescript": "latest",
    "typescript-eslint": "latest"
  }
}
