{
  "name": "@grayarrow/core-utils",
  "version": "1.0.61",
  "description": "Core helper utilities for supporting TypeScript and JavaScript projects.",
  "private": false,
  "type": "module",
  "exports": "./dist/index.mjs",
  "scripts": {
    "build": "rm -fr dist/* && tsc",
    "buildb4": "rm -fr dist/* && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && mv dist/mjs/index.js dist/mjs/index.mjs && ./fixup",
    "clean": "rm -rf coverage dist/* node_modules yarn.lock package-lock.json",
    "clean:build": "yarn clean && yarn && yarn build",
    "coverage": "jest --coverage",
    "lint": "eslint ./src/ --fix",
    "prepare": "husky install",
    "test:watch": "jest --watch",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grayarrow/core-utils.git"
  },
  "engines": {
    "node": ">=12.0"
  },
  "keywords": [
    "GrayArrow",
    "utilities",
    "TypeScript"
  ],
  "author": "David Dietrich",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/grayarrow/core-utils/issues"
  },
  "homepage": "https://github.com/grayarrow/core-utils#readme",
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-no-for-of-loops": "^1.0.1",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "jest": "^29.4.2",
    "lint-staged": "^13.0.4",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --cache-location .eslintcache --fix"
  },
  "dependencies": {
    "axios": "^1.3.2",
    "crypto-js": "^4.1.1"
  }
}
