{
  "name": "@grayarrow/core-utils",
  "version": "3.0.0",
  "description": "Core helper utilities for supporting TypeScript and JavaScript projects.",
  "private": false,
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/mjs/index.mjs",
  "types": "./dist/mjs/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/mjs/index.mjs",
      "types": "./dist/mjs/index.d.ts"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "buildesm": "rimraf dist && tsc",
    "build": "rimraf dist && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && mv dist/mjs/index.js dist/mjs/index.mjs && ./fixup",
    "clean": "rimraf coverage dist node_modules yarn.lock package-lock.json",
    "clean:build": "npm run clean && npm install && npm run build",
    "coverage": "jest --coverage",
    "lint": "eslint ./src/ --fix",
    "prepare": "husky install",
    "test:watch": "jest --watch",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grayarrow/core-utils.git"
  },
  "engines": {
    "node": ">=16.0"
  },
  "keywords": [
    "GrayArrow",
    "utilities",
    "TypeScript"
  ],
  "author": "David Dietrich",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/grayarrow/core-utils/issues"
  },
  "homepage": "https://github.com/grayarrow/core-utils#readme",
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mssql": "^9.1.5",
    "@types/node": "^20.12.7",
    "@types/uuid": "^9.0.8",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.57.0",
    "eslint-config-love": "^43.1.0",
    "eslint-plugin-no-for-of-loops": "^1.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "msw": "^2.3.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "latest"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --cache-location .eslintcache --fix"
  },
  "dependencies": {
    "@azure/eventgrid": "^5.3.0",
    "@azure/functions": "^3.0.0",
    "@azure/identity": "^4.0.1",
    "@azure/keyvault-secrets": "^4.8.0",
    "applicationinsights": "^2.9.5",
    "axios": "^1.6.8",
    "crypto-js": "^4.2.0",
    "express": "^4.19.1",
    "jsonwebtoken": "^9.0.2",
    "mssql": "^10.0.2",
    "uuid": "^9.0.1",
    "winston": "^3.12.0",
    "winston-daily-rotate-file": "^5.0.0",
    "xml2js": "^0.6.2"
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
